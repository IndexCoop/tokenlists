{
  "name": "@indexcoop/tokenlists",
  "version": "1.31.1",
  "description": "Token lists used by Index Coop",
  "main": "./build/cjs/index.js",
  "types": "./build/cjs/index.d.ts",
  "module": "./build/esm/index.js",
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "rimraf build",
    "commit": "git-cz",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "fix": "gts fix",
    "prepare": "husky install && npm run build",
    "posttest": "npm run lint",
    "build": "npm run compile:esm && npm run compile:cjs",
    "semantic-release": "semantic-release --branches main",
    "acp": "git add . && npm run commit && git push"
  },
  "keywords": [
    "npm",
    "example",
    "react",
    "hooks",
    "commonjs",
    "es6"
  ],
  "homepage": "https://github.com/IndexCoop/tokenlists",
  "repository": {
    "type": "git",
    "url": "https://github.com/IndexCoop/tokenlists"
  },
  "bugs": {
    "url": "https://github.com/IndexCoop/tokenlists/issues"
  },
  "author": {
    "name": "0xModene",
    "email": "modene@indexcoop.com",
    "url": "https://indexcoop.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "cz-conventional-changelog": "^3.3.0",
    "gts": "^3",
    "husky": "^8.0.1",
    "jest": "^28.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3",
    "semantic-release": "^19.0.5",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
