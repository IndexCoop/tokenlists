{
  "name": "@indexcoop/tokenlists",
  "version": "1.37.0",
  "description": "Token lists used by Index Coop",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "rimraf build",
    "commit": "git-cz",
    "fix": "gts fix",
    "prepare": "husky install && npm run build",
    "posttest": "npm run lint",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "npm run build -- --watch src",
    "semantic-release": "semantic-release --branches main",
    "acp": "git add . && npm run commit && git push"
  },
  "keywords": [
    "indexcoop",
    "tokenlists",
    "index",
    "coop"
  ],
  "homepage": "https://github.com/IndexCoop/tokenlists",
  "repository": {
    "type": "git",
    "url": "https://github.com/IndexCoop/tokenlists"
  },
  "bugs": {
    "url": "https://github.com/IndexCoop/tokenlists/issues"
  },
  "author": "Index Coop",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "cz-conventional-changelog": "^3.3.0",
    "gts": "^3",
    "husky": "^8.0.1",
    "jest": "^28.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3",
    "semantic-release": "^19.0.5",
    "ts-jest": "^28.0.8",
    "tsup": "^8.0.1",
    "typescript": "^4.8.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
